{
  "name": "inspect-web",
  "version": "1.0.0",
  "description": "Template project for a node express web server",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/inspect-web"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c '[ -f /KTH_NODEJS ] && cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "dependencies": {
