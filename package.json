  "scripts": {
    "//": "Gulp installs css and javascript",
    "test": "echo \"ok\"",
    "docker": "npm install --development && npm run build && npm prune --production",
    "webpackProd": "npm run build && echo \"Deprecated, use 'npm run build' instead\"",
    "vendorProd": "echo \"Deprecated, use 'npm run build' instead\"",
    "buildConfig": "cross-env NODE_PATH=. NODE_ENV=development node ./buildConfig.js",
    "postinstall": "npm run buildConfig",
    "build": "gulp build --production",
    "start": "NODE_ENV=development concurrently --kill-others \"nodemon app.js\" \"gulp watch\""
  },
